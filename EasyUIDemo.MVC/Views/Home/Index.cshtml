@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>EasyUIDemo</title>
    <link href="~/Content/EasyUI/themes/default/easyui.css" rel="stylesheet" />
    <link href="~/Content/EasyUI/themes/icon.css" rel="stylesheet" />
    <link href="~/Content/admin.css" rel="stylesheet" />
    <script src="~/Content/EasyUI/jquery.min.js"></script>
    <script src="~/Content/EasyUI/jquery.easyui.min.js"></script>
    <script src="~/Scripts/common.js"></script>
</head>
<body class="easyui-layout" data-options="fit:true,scroll:'no'">
    <div data-options="region:'north',border:false" style="height: 74px; background: #2596ea; padding: 10px">
        EasyUIDemo
    </div>
    <div data-options="region:'west',split:true,title:'导航菜单'" style="width: 170px;">
        <div class="easyui-accordion" data-options="fit:true,border:false">
            <div title="系统设置" data-options="selected:true">
                <div style="margin: 5px">
                    <ul class="tree easyui-tree" data-options="animate:true,lines:true">
                        <li data-options="iconCls:'icon-joystick'">
                            <span>基本设置</span>
                            <ul>
                                <li data-options="iconCls:'icon-user'">
                                    <a onclick="addTab('项目配置','@Url.Content("~/User/User_List")','')"><span>项目配置</span></a>
                                </li>
                                <li data-options="iconCls:'icon-user'">
                                    <a onclick="addTab('楼盘管理','@Url.Content("~/User/User_List")','')"><span>楼盘管理</span></a>
                                </li>
                                <li data-options="iconCls:'icon-user'">
                                    <a onclick="addTab('施教区管理','@Url.Content("~/User/User_List")','')"><span>施教区管理</span></a>
                                </li>
                            </ul>
                        </li>
                        <li data-options="iconCls:'icon-joystick'">
                            <span>学位管理</span>
                            <ul>
                                <li data-options="iconCls:'icon-user'">
                                    <a onclick="addTab('学位信息导入','@Url.Content("~/Car/CarList")','')"><span>车牌管理</span></a>
                                </li>
                                <li data-options="iconCls:'icon-user'">
                                    <a onclick="addTab('入学申请信息','@Url.Content("~/Car/CarList")','')"><span>车牌管理</span></a>
                                </li>
                                <li data-options="iconCls:'icon-user'">
                                    <a onclick="addTab('已提交学位管理','@Url.Content("~/Car/CarList")','')"><span>车牌管理</span></a>
                                </li>
                                <li data-options="iconCls:'icon-user'">
                                    <a onclick="addTab('学位信息维护','@Url.Content("~/Car/CarList")','')"><span>车牌管理</span></a>
                                </li>
                                <li data-options="iconCls:'icon-user'">
                                    <a onclick="addTab('学位库管理','@Url.Content("~/Car/CarList")','')"><span>车牌管理</span></a>
                                </li>
                            </ul>
                        </li>
                        <li data-options="iconCls:'icon-joystick'">
                            <span>学位信息查询</span>
                            <ul>
                                <li data-options="iconCls:'icon-user'">
                                    <a onclick="addTab('学位查询','@Url.Content("~/User/User_List")','')"><span>项目配置</span></a>
                                </li>
                                <li data-options="iconCls:'icon-user'">
                                    <a onclick="addTab('学位查询日志','@Url.Content("~/User/User_List")','')"><span>楼盘管理</span></a>
                                </li>
                            </ul>
                        </li>
                        <li data-options="iconCls:'icon-joystick'">
                            <span>统计报表</span>
                            <ul>
                                <li data-options="iconCls:'icon-user'">
                                    <a onclick="addTab('区域施教区统计报表','@Url.Content("~/User/User_List")','')"><span>区域施教区统计报表</span></a>
                                </li>
                                <li data-options="iconCls:'icon-user'">
                                    <a onclick="addTab('区域综合统计报表','@Url.Content("~/User/User_List")','')"><span>区域综合统计报表</span></a>
                                </li>
                                <li data-options="iconCls:'icon-user'">
                                    <a onclick="addTab('区域择校生统计报表','@Url.Content("~/User/User_List")','')"><span>区域择校生统计报表</span></a>
                                </li>
                                <li data-options="iconCls:'icon-user'">
                                    <a onclick="addTab('区域地段生统计报表','@Url.Content("~/User/User_List")','')"><span>区域地段生统计报表</span></a>
                                </li>
                            </ul>
                        </li>
                        <li data-options="iconCls:'icon-joystick'">
                            <span>系统管理</span>
                            <ul>
                                <li data-options="iconCls:'icon-user'">
                                    <a onclick="addTab('用户管理','@Url.Content("~/Car/CarList")','')"><span>用户管理</span></a>
                                </li>
                                 <li data-options="iconCls:'icon-user'">
                                    <a onclick="addTab('角色管理','@Url.Content("~/Car/CarList")','')"><span>角色管理</span></a>
                                </li>
                                <li data-options="iconCls:'icon-user'">
                                    <a onclick="addTab('模块管理','@Url.Content("~/Car/CarList")','')"><span>模块管理</span></a>
                                </li>
                                 <li data-options="iconCls:'icon-user'">
                                    <a onclick="addTab('操作管理','@Url.Content("~/Car/CarList")','')"><span>操作管理</span></a>
                                </li>
                                 <li data-options="iconCls:'icon-user'">
                                    <a onclick="addTab('权限管理','@Url.Content("~/Car/CarList")','')"><span>权限管理</span></a>
                                </li>
                                 <li data-options="iconCls:'icon-user'">
                                    <a onclick="addTab('菜单管理','@Url.Content("~/Car/CarList")','')"><span>菜单管理</span></a>
                                </li>
                                 <li data-options="iconCls:'icon-user'">
                                    <a onclick="addTab('日志管理','@Url.Content("~/Car/CarList")','')"><span>日志管理</span></a>
                                </li>
                            </ul>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
    <div data-options="region:'south',border:false" style="height: 23px;">
        <div class="footer">EasyUIDemo</div>
    </div>
    <div data-options="region:'center'">
        <div id="tabs" class="easyui-tabs" data-options="tools:'#tab-tools',fit:true,border:false" >
            <div title="主页" data-options="iconCls:'icon-house'" style="padding: 10px;">
                @*<iframe scrolling="auto" frameborder="0"  src="Home/GetView?viewPara=../User/User_Add" style="width:100%;height:100%;"></iframe>*@
                <iframe scrolling="auto" frameborder="0"  src="@Url.Content("~/Home/Home/")" style="width:100%;height:100%;"></iframe>
            </div>
        </div>
        <div id="tab-tools">
            <a href="javascript:void(0)" class="easyui-linkbutton" data-options="plain:true,iconCls:'icon-add'" onclick="addTab('主页','Home/GetView?viewPara=Home','icon-house')"></a>
            <a href="javascript:void(0)" class="easyui-linkbutton" data-options="plain:true,iconCls:'icon-remove'" onclick="removeTab()"></a>
        </div>
    </div>
    <div id="mm" class="easyui-menu" style="width: 150px;">
        <div id="refresh" data-options="iconCls:'icon-arrow_refresh'">刷新</div>
        <div class="menu-sep"></div>
        <div id="close">关闭</div>
        <div id="closeall">全部关闭</div>
        <div id="closeother">除此之外全部关闭</div>
        <div class="menu-sep"></div>
        <div id="closeright">当前页右侧全部关闭</div>
        <div id="closeleft">当前页左侧全部关闭</div>
        <div class="menu-sep"></div>
        <div id="exit">退出</div>
    </div>
</body>
</html>
